<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 
 	<class name="com.bridgeIt.fundooNotes.model.UserModel" table="UserDetails">
 		<id name="id" column="sl_no">
 			<generator class="native"></generator>
 		</id>
 		<property name="rEmail" column="user_email" type="string"/>
 		<property name="rPassWord" column="user_passWord"/>
 		<property name="rPhnNo" column="user_phno"/>
 	</class>
   

</hibernate-mapping>
